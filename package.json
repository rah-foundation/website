    "is-ci": "^1.0.9",
    "json-loader": "^0.5.4",
    "karma": "^1.1.1",
    "karma-chrome-launcher": "^2.1.1",
    "karma-mocha": "^1.1.1",
    "karma-webpack": "^2.0.3",
    "less": "^2.7.1",
